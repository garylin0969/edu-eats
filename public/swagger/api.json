            "email": ""
        }
    },
    "host": "fatraceschool.k12ea.gov.tw",
    "basePath": "/",
    "tags": [
        {
            "name": "county",
            "description": "縣市資料相關操作"
        },
        {
            "name": "area",
            "description": "區域資料相關操作"
        },
        {
            "name": "school",
            "description": "學校資料相關操作"
        },
        {
            "name": "canteen",
            "description": "餐廳資料相關操作"
        }
    ],
    "schemes": [
        "https"
