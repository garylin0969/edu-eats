    "paths": {
        "/county": {
            "get": {
                "tags": [
                    "county"
                ],
                "summary": "取得縣市列表",
                "description": "取得所有縣市的資料列表",
                "operationId": "getCounties",
                "produces": [
                    "application/json"
                ],
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "成功取得縣市資料",
                        "schema": {
                            "$ref": "#/definitions/CountyResponse"
                        }
                    },
                    "400": {
                        "description": "請求參數錯誤"
                    },
                    "500": {
                        "description": "伺服器內部錯誤"
                    }
                }
            }
        },
        "/area": {
            "get": {
                "tags": [
                    "area"
                ],
                "summary": "查詢區域資料",
                "description": "根據縣市ID查詢該縣市下的所有區域資料",
                "operationId": "getAreas",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "CountyId",
                        "in": "query",
                        "description": "縣市ID",
                        "required": true,
                        "type": "integer",
                        "example": 19
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功取得區域資料",
                        "schema": {
                            "$ref": "#/definitions/AreaResponse"
                        }
                    },
                    "400": {
                        "description": "請求參數錯誤"
                    },
                    "500": {
                        "description": "伺服器內部錯誤"
                    }
                }
            }
        },
        "/school": {
            "get": {
                "tags": [
                    "school"
                ],
                "summary": "搜尋學校資料",
                "description": "https://fatraceschool.k12ea.gov.tw/school?CountyId=19&AreaId=131398&SchoolType=5",
                "operationId": "searchSchools",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "CountyId",
                        "in": "query",
                        "description": "縣市ID",
                        "required": false,
                        "type": "string",
                        "example": "19"
                    },
                    {
                        "name": "AreaId",
                        "in": "query",
                        "description": "區域ID",
                        "required": false,
                        "type": "string",
                        "example": "131398"
                    },
                    {
                        "name": "SchoolType",
                        "in": "query",
                        "description": "學校院所類型(1:幼兒園,2:國小,3:國中,4:高中職/五專,5:大專院校)",
                        "required": false,
                        "type": "string",
                        "example": "5"
                    },
                    {
                        "name": "SchoolName",
                        "in": "query",
                        "description": "學校名稱 (支援模糊搜尋)",
                        "required": false,
                        "type": "string",
                        "example": "輔仁"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功取得學校資料",
                        "schema": {
                            "$ref": "#/definitions/SchoolResponse"
                        }
                    },
                    "400": {
                        "description": "請求參數錯誤"
                    },
                    "500": {
                        "description": "伺服器內部錯誤"
                    }
                }
            }
        },
        "/school/{schoolId}": {
            "get": {
                "tags": [
                    "school"
                ],
                "summary": "根據學校ID取得學校詳細資料",
                "description": "根據學校ID取得單一學校的詳細資料",
                "operationId": "getSchoolById",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "schoolId",
                        "in": "path",
                        "description": "學校ID",
                        "required": true,
                        "type": "integer",
                        "format": "int64",
                        "example": 64741889
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功取得學校詳細資料",
                        "schema": {
                            "$ref": "#/definitions/SchoolDetailResponse"
                        }
                    },
                    "400": {
                        "description": "請求參數錯誤"
                    },
                    "404": {
                        "description": "學校不存在"
                    },
                    "500": {
                        "description": "伺服器內部錯誤"
                    }
                }
            }
        },
        "/canteen": {
            "get": {
                "tags": [
                    "canteen"
                ],
                "summary": "搜尋學校餐廳資料",
                "description": "根據學校ID搜尋該學校的餐廳資料，可選擇性指定校區和時間期間",
                "operationId": "searchCanteens",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "SchoolId",
                        "in": "query",
                        "description": "學校ID",
                        "required": true,
                        "type": "integer",
                        "example": 64741748
                    },
                    {
                        "name": "SFStreetId",
                        "in": "query",
                        "description": "校區ID",
                        "required": false,
                        "type": "integer",
                        "example": 71
                    },
                    {
                        "name": "period",
                        "in": "query",
                        "description": "查詢時間期間 (格式: YYYY-MM-DD)",
                        "required": false,
                        "type": "string",
                        "example": "2025-06-24"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功取得餐廳資料",
                        "schema": {
                            "$ref": "#/definitions/CanteenResponse"
                        }
                    },
                    "400": {
                        "description": "請求參數錯誤"
                    },
                    "500": {
                        "description": "伺服器內部錯誤"
                    }
                }
            }
        },
        "/kitchen/logo/{kitchenId}": {
            "get": {
                "tags": [
                    "canteen"
                ],
                "summary": "取得廚房/餐廳 Logo 圖片",
                "description": "根據廚房ID取得對應的 Logo 圖片",
                "operationId": "getKitchenLogo",
                "produces": [
                    "image/jpeg",
                    "image/png",
                    "image/gif",
                    "image/webp"
                ],
                "parameters": [
                    {
                        "name": "kitchenId",
                        "in": "path",
                        "description": "廚房ID",
                        "required": true,
                        "type": "string",
                        "example": "15833"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功取得 Logo 圖片",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "404": {
                        "description": "找不到對應的 Logo 圖片"
                    },
                    "500": {
                        "description": "伺服器內部錯誤"
                    }
                }
            }
        },
        "/canteen/meal": {
            "get": {
                "tags": [
                    "canteen"
                ],
                "summary": "查詢餐廳菜單",
                "description": "根據學校ID和餐廳ID查詢指定時間期間的菜單資料",
                "operationId": "getCanteenMeal",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "SchoolId",
                        "in": "query",
                        "description": "學校ID",
                        "required": true,
                        "type": "integer",
                        "example": 64741748
                    },
                    {
                        "name": "RestaurantId",
                        "in": "query",
                        "description": "餐廳ID",
                        "required": true,
                        "type": "integer",
                        "example": 100133
                    },
                    {
                        "name": "period",
                        "in": "query",
                        "description": "查詢時間期間 (格式: YYYY-MM-DD,YYYY-MM-DD)",
                        "required": true,
                        "type": "string",
                        "example": "2025-06-24,2025-06-24"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功取得菜單資料",
                        "schema": {
                            "$ref": "#/definitions/MealResponse"
                        }
                    },
                    "400": {
                        "description": "請求參數錯誤"
                    },
                    "500": {
                        "description": "伺服器內部錯誤"
                    }
                }
            }
        },
        "/dish/pic/{restaurantId}-{dishId}": {
            "get": {
                "tags": [
                    "canteen"
                ],
                "summary": "取得餐點圖片",
                "description": "根據餐廳ID和菜品ID取得對應的餐點圖片",
                "operationId": "getDishPicture",
                "produces": [
                    "image/jpeg",
                    "image/png",
                    "image/gif",
                    "image/webp"
                ],
                "parameters": [
                    {
                        "name": "restaurantId",
                        "in": "path",
                        "description": "餐廳ID",
                        "required": true,
                        "type": "integer",
                        "example": 100133
                    },
                    {
                        "name": "dishId",
                        "in": "path",
                        "description": "菜品ID",
                        "required": true,
                        "type": "integer",
                        "example": 1185559
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功取得餐點圖片",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "404": {
                        "description": "找不到對應的餐點圖片"
                    },
                    "500": {
                        "description": "伺服器內部錯誤"
                    }
                }
            }
        }
    },
